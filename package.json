{
  "name": "hotballoon",
  "jest": {
    "verbose": true,
    "globals": {
      "__ASSERT__": true
    },
    "testMatch": [
      "<rootDir>/__tests__/**/?(*.)+(test).js"
    ],
    "testEnvironment": "jsdom"
  },
  "description": "Small JavaScript library for building interfaces by Flexio",
  "main": "index.js",
  "module": "index.js",
  "version": "1.0.1",
  "license": "Apache-2.0",
  "scripts": {
    "doc": "yarn jsdoc -d ./docs/ -p -r ./src/ ",
