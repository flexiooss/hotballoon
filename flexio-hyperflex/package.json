{
  "name": "flexio-hyperflex",
  "description": "Helpers to create DOM elements",
  "module": "index.js",
  "main": "index.js",
  "author": "Thomas Chatelain (https://github.com/TomAchT)",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "hbshed dev --entry src/test/dev/dev.js --server-config local"
  },
  "hotballoon-shed": {
    "module": {
      "parent": {
        "name": "@flexio-oss/hotballoon"
      },
      "dependencies": [
        "@flexio-oss/js-commons-bundle"
      ],
      "devDependencies": [
        "@flexio-oss/code-altimeter-js"
      ]
    },
    "build": {
      "builder": "webpack5"
    },
    "test": {
      "tester": "code-altimeter-js",
      "path": "src/test"
    },
    "generate-sources": {
      "value-objects": {
        "extension": ".spec"
      }
    }
  },
  "peerDependencies": {
    "@flexio-oss/code-altimeter-js": "1.3.0",
    "@flexio-oss/hotballoon": "0.30.0-dev",
    "@flexio-oss/js-commons-bundle": "1.28.0-dev"
  }
}
